<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    >
    <Color x:Key="BroBorderColor">#FF888888</Color>
    <!--<Color x:Key="BroBorderColor">Red</Color>-->
 
    <SolidColorBrush x:Key="BroBorder" Color="{StaticResource BroBorderColor}" />
    <SolidColorBrush x:Key="BroForeground" Color="#FF171717" options:Freeze="True" />
    <SolidColorBrush x:Key="BroBackground" Color="{StaticResource WhiteColor}" options:Freeze="True" />
    <SolidColorBrush x:Key="BroContentBackground" Color="{StaticResource ContentBackColor}" options:Freeze="True" />
    <SolidColorBrush x:Key="BroAlternative" Color="{StaticResource AlternativeColor}" options:Freeze="True" />
    <SolidColorBrush x:Key="BroFrameSelection" Color="Red" options:Freeze="True"/>
    <SolidColorBrush x:Key="BroFrameHover" Color="Black" options:Freeze="True"/>

    <SolidColorBrush x:Key="WindowTitleColorBrush" Color="{StaticResource ContentBackColor}" options:Freeze="True"/>
    <SolidColorBrush x:Key="CheckBoxBrush" Color="{StaticResource BroBorderColor}" options:Freeze="True" />
    <SolidColorBrush x:Key="TextBoxBorderBrush" Color="{StaticResource BroBorderColor}" options:Freeze="True" />
</ResourceDictionary>